<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="DocRepository" namespace="DocRepository.Models">
  <class name="Documents" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" type="int">
      <generator class="native" />
    </id>
    <property name="DocName" />
    <property name="DocDate" type="DateTime" />
    <property name="DocAuthor" />
    <property name="DocFileName" />
  </class>
</hibernate-mapping>